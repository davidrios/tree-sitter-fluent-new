{
  "name": "tree-sitter-fluent",
  "version": "0.2.0",
  "description": "Fluent grammar for tree-sitter",
  "repository": "https://github.com/tree-sitter/tree-sitter-fluent",
  "license": "MIT",
  "author": {
    "name": "David Rios",
    "email": "david.rios.gomes@gmail.com"
  },
  "main": "bindings/node",
  "types": "bindings/node",
  "keywords": [
    "incremental",
    "parsing",
    "tree-sitter",
    "fluent"
  ],
  "files": [
    "grammar.js",
    "tree-sitter.json",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**",
    "*.wasm"
  ],
  "dependencies": {
    "node-addon-api": "8.5.0",
    "node-gyp-build": "4.8.4"
  },
  "devDependencies": {
    "@eslint/js": "9.39.2",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-config-treesitter": "1.0.2",
    "eslint-plugin-prettier": "5.5.5",
    "globals": "17.0.0",
    "prebuildify": "6.0.1",
    "prettier": "3.7.4",
    "tree-sitter": "0.25.0",
    "tree-sitter-cli": "0.26.3"
  },
  "scripts": {
    "install": "node-gyp-build",
    "prestart": "tree-sitter build --wasm",
    "start": "tree-sitter playground",
    "test": "node --test bindings/node/*_test.js",
    "lint": "eslint . --fix --cache",
    "format": "prettier --write --experimental-cli ."
  }
}
