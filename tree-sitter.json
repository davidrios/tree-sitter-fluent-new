{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "fluent",
      "camelcase": "Fluent",
      "title": "Fluent",
      "scope": "source.fluent",
      "file-types": [
        "ftl"
      ],
      "injection-regex": "^fluent$",
      "class-name": "TreeSitterFluent"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Fluent grammar for tree-sitter",
    "authors": [
      {
        "name": "David Rios",
        "email": "david.rios.gomes@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-fluent"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}