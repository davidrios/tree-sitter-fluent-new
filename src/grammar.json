{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "fluent",
  "rules": {
    "source": {
      "type": "REPEAT",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "blank_lines"
          },
          {
            "type": "SYMBOL",
            "name": "comment"
          },
          {
            "type": "SYMBOL",
            "name": "group_comment"
          },
          {
            "type": "SYMBOL",
            "name": "file_comment"
          },
          {
            "type": "SYMBOL",
            "name": "message"
          },
          {
            "type": "SYMBOL",
            "name": "message_with_comment"
          }
        ]
      }
    },
    "whitespaces": {
      "type": "PATTERN",
      "value": "[ \t]+"
    },
    "blank_lines": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "REPEAT1",
        "content": {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "new_lines"
            },
            {
              "type": "SYMBOL",
              "name": "whitespaces"
            }
          ]
        }
      }
    },
    "new_line": {
      "type": "PREC_LEFT",
      "value": 1,
      "content": {
        "type": "STRING",
        "value": "\n"
      }
    },
    "new_lines": {
      "type": "PATTERN",
      "value": "\\n+"
    },
    "comment_content": {
      "type": "PATTERN",
      "value": ".+"
    },
    "comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "#"
        },
        {
          "type": "SYMBOL",
          "name": "comment_content"
        }
      ]
    },
    "group_comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "##"
        },
        {
          "type": "SYMBOL",
          "name": "comment_content"
        }
      ]
    },
    "file_comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "###"
        },
        {
          "type": "SYMBOL",
          "name": "comment_content"
        }
      ]
    },
    "message_with_comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "REPEAT1",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "comment"
              },
              {
                "type": "STRING",
                "value": "\n"
              }
            ]
          }
        },
        {
          "type": "SYMBOL",
          "name": "message"
        }
      ]
    },
    "prec_whitespaces": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": 10,
        "content": {
          "type": "PATTERN",
          "value": "[ \t]+"
        }
      }
    },
    "message": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "identifier"
            },
            {
              "type": "SYMBOL",
              "name": "term"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "prec_whitespaces"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "assignment"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "prec_whitespaces"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "message_value"
        }
      ]
    },
    "_identifier": {
      "type": "PATTERN",
      "value": "[a-z_][a-z0-9_-]*"
    },
    "identifier": {
      "type": "SYMBOL",
      "name": "_identifier"
    },
    "term": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "-"
        },
        {
          "type": "SYMBOL",
          "name": "_identifier"
        }
      ]
    },
    "assignment": {
      "type": "STRING",
      "value": "="
    },
    "message_value": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "pattern"
            },
            {
              "type": "SYMBOL",
              "name": "message_continuation"
            }
          ]
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "message_continuation"
          }
        }
      ]
    },
    "text": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": 1,
        "content": {
          "type": "PATTERN",
          "value": "[^\\n{}]+"
        }
      }
    },
    "pattern": {
      "type": "REPEAT1",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "text"
          },
          {
            "type": "SYMBOL",
            "name": "placeable"
          }
        ]
      }
    },
    "message_continuation": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "\\n[ \t]+"
        },
        {
          "type": "SYMBOL",
          "name": "pattern"
        }
      ]
    },
    "placeable": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "whitespaces"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "quoted_text"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "whitespaces"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "quoted_text": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\""
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "quoted_escaped"
              },
              {
                "type": "PATTERN",
                "value": "[^\\\\\"]+"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "\""
        }
      ]
    },
    "quoted_escaped": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "\\u"
            },
            {
              "type": "SYMBOL",
              "name": "_hexdigit"
            },
            {
              "type": "SYMBOL",
              "name": "_hexdigit"
            },
            {
              "type": "SYMBOL",
              "name": "_hexdigit"
            },
            {
              "type": "SYMBOL",
              "name": "_hexdigit"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "\\U"
            },
            {
              "type": "SYMBOL",
              "name": "_hexdigit"
            },
            {
              "type": "SYMBOL",
              "name": "_hexdigit"
            },
            {
              "type": "SYMBOL",
              "name": "_hexdigit"
            },
            {
              "type": "SYMBOL",
              "name": "_hexdigit"
            },
            {
              "type": "SYMBOL",
              "name": "_hexdigit"
            },
            {
              "type": "SYMBOL",
              "name": "_hexdigit"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "\\\""
        },
        {
          "type": "STRING",
          "value": "\\\\"
        }
      ]
    },
    "_hexdigit": {
      "type": "PATTERN",
      "value": "[0-9a-fA-F]"
    }
  },
  "extras": [],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}