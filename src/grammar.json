{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "fluent",
  "rules": {
    "source": {
      "type": "REPEAT",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "comment"
          },
          {
            "type": "SYMBOL",
            "name": "group_comment"
          },
          {
            "type": "SYMBOL",
            "name": "file_comment"
          },
          {
            "type": "SYMBOL",
            "name": "message"
          },
          {
            "type": "SYMBOL",
            "name": "message_with_doc_comment"
          },
          {
            "type": "SYMBOL",
            "name": "term"
          },
          {
            "type": "SYMBOL",
            "name": "term_with_doc_comment"
          },
          {
            "type": "SYMBOL",
            "name": "blank_lines"
          }
        ]
      }
    },
    "whitespaces": {
      "type": "PATTERN",
      "value": " +"
    },
    "comment_content": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "[^\\n]+"
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "# "
        },
        {
          "type": "SYMBOL",
          "name": "comment_content"
        }
      ]
    },
    "group_comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "## "
        },
        {
          "type": "SYMBOL",
          "name": "comment_content"
        }
      ]
    },
    "file_comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "### "
        },
        {
          "type": "SYMBOL",
          "name": "comment_content"
        }
      ]
    },
    "message": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "id",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "whitespaces"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "assignment"
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "FIELD",
                  "name": "value",
                  "content": {
                    "type": "SYMBOL",
                    "name": "pattern"
                  }
                },
                {
                  "type": "PREC",
                  "value": 1,
                  "content": {
                    "type": "SYMBOL",
                    "name": "pattern_skip"
                  }
                }
              ]
            },
            {
              "type": "FIELD",
              "name": "attribute",
              "content": {
                "type": "REPEAT",
                "content": {
                  "type": "SYMBOL",
                  "name": "attribute"
                }
              }
            }
          ]
        }
      ]
    },
    "message_with_doc_comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "REPEAT1",
          "content": {
            "type": "PREC",
            "value": 1,
            "content": {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "comment"
                }
              ]
            }
          }
        },
        {
          "type": "SYMBOL",
          "name": "message"
        }
      ]
    },
    "attribute": {
      "type": "SEQ",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "."
            },
            {
              "type": "FIELD",
              "name": "id",
              "content": {
                "type": "SYMBOL",
                "name": "identifier"
              }
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "whitespaces"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "assignment"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "value",
              "content": {
                "type": "SYMBOL",
                "name": "pattern"
              }
            },
            {
              "type": "PREC",
              "value": 1,
              "content": {
                "type": "SYMBOL",
                "name": "pattern_skip"
              }
            }
          ]
        }
      ]
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-zA-Z][a-zA-Z0-9_-]*"
    },
    "term": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "id",
          "content": {
            "type": "SYMBOL",
            "name": "term_identifier"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "whitespaces"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "assignment"
        },
        {
          "type": "FIELD",
          "name": "value",
          "content": {
            "type": "SYMBOL",
            "name": "pattern"
          }
        }
      ]
    },
    "term_with_doc_comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "REPEAT1",
          "content": {
            "type": "PREC",
            "value": 1,
            "content": {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "comment"
                }
              ]
            }
          }
        },
        {
          "type": "SYMBOL",
          "name": "term"
        }
      ]
    },
    "term_identifier": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "-"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          },
          "named": false,
          "value": "identifier"
        }
      ]
    },
    "assignment": {
      "type": "STRING",
      "value": "="
    },
    "variable": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "$"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          },
          "named": false,
          "value": "identifier"
        }
      ]
    },
    "pattern": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "pattern_start"
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "pattern_pure_text"
              },
              {
                "type": "SYMBOL",
                "name": "placeable"
              }
            ]
          }
        },
        {
          "type": "SYMBOL",
          "name": "pattern_end"
        }
      ]
    },
    "expression": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "number"
        },
        {
          "type": "SYMBOL",
          "name": "quoted_text"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          },
          "named": true,
          "value": "message_id"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "term_identifier"
          },
          "named": true,
          "value": "term_id"
        },
        {
          "type": "SYMBOL",
          "name": "variable"
        }
      ]
    },
    "selector_key": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "SYMBOL",
          "name": "number"
        }
      ]
    },
    "selector_variant": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "\\*?\\[[ \\n]*"
        },
        {
          "type": "FIELD",
          "name": "key",
          "content": {
            "type": "SYMBOL",
            "name": "selector_key"
          }
        },
        {
          "type": "PATTERN",
          "value": "[\\n ]*\\]"
        },
        {
          "type": "FIELD",
          "name": "value",
          "content": {
            "type": "SYMBOL",
            "name": "pattern"
          }
        }
      ]
    },
    "selectors": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": " *-> *\\n[ \\n]*"
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "SYMBOL",
            "name": "selector_variant"
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "placeable": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "\\{[ \\n]*"
        },
        {
          "type": "SYMBOL",
          "name": "expression"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "selectors"
            },
            {
              "type": "PATTERN",
              "value": "[ \\n]*\\}"
            }
          ]
        }
      ]
    },
    "number": {
      "type": "PATTERN",
      "value": "\\d\\d*(\\.\\d+)?"
    },
    "quoted_text": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\""
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "quoted_escaped"
              },
              {
                "type": "PATTERN",
                "value": "[^\\\\\"]+"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "\""
        }
      ]
    },
    "quoted_escaped": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "\\u"
            },
            {
              "type": "SYMBOL",
              "name": "_hexdigit"
            },
            {
              "type": "SYMBOL",
              "name": "_hexdigit"
            },
            {
              "type": "SYMBOL",
              "name": "_hexdigit"
            },
            {
              "type": "SYMBOL",
              "name": "_hexdigit"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "\\U"
            },
            {
              "type": "SYMBOL",
              "name": "_hexdigit"
            },
            {
              "type": "SYMBOL",
              "name": "_hexdigit"
            },
            {
              "type": "SYMBOL",
              "name": "_hexdigit"
            },
            {
              "type": "SYMBOL",
              "name": "_hexdigit"
            },
            {
              "type": "SYMBOL",
              "name": "_hexdigit"
            },
            {
              "type": "SYMBOL",
              "name": "_hexdigit"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "\\\""
        },
        {
          "type": "STRING",
          "value": "\\\\"
        }
      ]
    },
    "_hexdigit": {
      "type": "PATTERN",
      "value": "[0-9a-fA-F]"
    }
  },
  "extras": [],
  "conflicts": [],
  "precedences": [],
  "externals": [
    {
      "type": "SYMBOL",
      "name": "pattern_start"
    },
    {
      "type": "SYMBOL",
      "name": "pattern_pure_text"
    },
    {
      "type": "SYMBOL",
      "name": "pattern_end"
    },
    {
      "type": "SYMBOL",
      "name": "pattern_skip"
    },
    {
      "type": "SYMBOL",
      "name": "blank_lines"
    }
  ],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}