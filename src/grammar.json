{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "fluent",
  "rules": {
    "source_file": {
      "type": "SEQ",
      "members": [
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_blank_line"
          }
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "_definition"
              },
              {
                "type": "SYMBOL",
                "name": "_line_break"
              },
              {
                "type": "REPEAT",
                "content": {
                  "type": "SYMBOL",
                  "name": "_blank_line"
                }
              }
            ]
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_definition"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "_definition": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "message"
        }
      ]
    },
    "_identifier": {
      "type": "PATTERN",
      "value": "[a-z_][a-z0-9_-]*"
    },
    "identifier": {
      "type": "SYMBOL",
      "name": "_identifier"
    },
    "term": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "-"
        },
        {
          "type": "SYMBOL",
          "name": "_identifier"
        }
      ]
    },
    "_line_break": {
      "type": "PATTERN",
      "value": "[\\u000A\\u000D]+"
    },
    "_inline_space": {
      "type": "PATTERN",
      "value": "[\\u0020\\u0009]+"
    },
    "_blank_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_inline_space"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "_line_break"
        }
      ]
    },
    "_hexdigit": {
      "type": "PATTERN",
      "value": "[0-9a-fA-F]"
    },
    "quoted_text": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\""
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_inline_space"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "STRING",
                "value": "\\\""
              },
              {
                "type": "SEQ",
                "members": [
                  {
                    "type": "STRING",
                    "value": "\\u"
                  },
                  {
                    "type": "SYMBOL",
                    "name": "_hexdigit"
                  },
                  {
                    "type": "SYMBOL",
                    "name": "_hexdigit"
                  },
                  {
                    "type": "SYMBOL",
                    "name": "_hexdigit"
                  },
                  {
                    "type": "SYMBOL",
                    "name": "_hexdigit"
                  }
                ]
              },
              {
                "type": "SEQ",
                "members": [
                  {
                    "type": "STRING",
                    "value": "\\U"
                  },
                  {
                    "type": "SYMBOL",
                    "name": "_hexdigit"
                  },
                  {
                    "type": "SYMBOL",
                    "name": "_hexdigit"
                  },
                  {
                    "type": "SYMBOL",
                    "name": "_hexdigit"
                  },
                  {
                    "type": "SYMBOL",
                    "name": "_hexdigit"
                  },
                  {
                    "type": "SYMBOL",
                    "name": "_hexdigit"
                  },
                  {
                    "type": "SYMBOL",
                    "name": "_hexdigit"
                  }
                ]
              },
              {
                "type": "STRING",
                "value": "\\\\"
              },
              {
                "type": "PATTERN",
                "value": "[^\\\\\"]*"
              }
            ]
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_inline_space"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "\""
        }
      ]
    },
    "placeable": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_inline_space"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "quoted_text"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_inline_space"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "text": {
      "type": "REPEAT1",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "PATTERN",
            "value": "[\t -\\[\\]-z|-퟿-�]+"
          },
          {
            "type": "SYMBOL",
            "name": "placeable"
          }
        ]
      }
    },
    "message": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_inline_space"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_inline_space"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "text"
        }
      ]
    }
  },
  "extras": [],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}