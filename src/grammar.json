{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "fluent",
  "rules": {
    "fluent_file": {
      "type": "REPEAT",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "comment_block"
          },
          {
            "type": "SYMBOL",
            "name": "group_comment"
          },
          {
            "type": "SYMBOL",
            "name": "file_comment"
          },
          {
            "type": "SYMBOL",
            "name": "message"
          },
          {
            "type": "SYMBOL",
            "name": "term"
          },
          {
            "type": "SYMBOL",
            "name": "with_doc_comment"
          },
          {
            "type": "SYMBOL",
            "name": "_blank_lines"
          },
          {
            "type": "SYMBOL",
            "name": "unfinished_line"
          }
        ]
      }
    },
    "comment_content": {
      "type": "PATTERN",
      "value": "[^\\n]+\\n"
    },
    "_base_comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "# "
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "comment_content"
          },
          "named": true,
          "value": "content"
        }
      ]
    },
    "_comment_block": {
      "type": "SEQ",
      "members": [
        {
          "type": "REPEAT1",
          "content": {
            "type": "SYMBOL",
            "name": "_base_comment"
          }
        }
      ]
    },
    "comment_block": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_comment_block"
        },
        {
          "type": "SYMBOL",
          "name": "_close_comment_block"
        }
      ]
    },
    "group_comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "## "
        },
        {
          "type": "SYMBOL",
          "name": "comment_content"
        }
      ]
    },
    "file_comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "### "
        },
        {
          "type": "SYMBOL",
          "name": "comment_content"
        }
      ]
    },
    "with_doc_comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "_comment_block"
          },
          "named": true,
          "value": "doc_comment_block"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "message"
            },
            {
              "type": "SYMBOL",
              "name": "term"
            }
          ]
        }
      ]
    },
    "message": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "id",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "PATTERN",
          "value": " *"
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "FIELD",
                  "name": "value",
                  "content": {
                    "type": "SYMBOL",
                    "name": "pattern"
                  }
                },
                {
                  "type": "SYMBOL",
                  "name": "_pattern_skip"
                }
              ]
            },
            {
              "type": "FIELD",
              "name": "attributes",
              "content": {
                "type": "ALIAS",
                "content": {
                  "type": "REPEAT",
                  "content": {
                    "type": "SYMBOL",
                    "name": "attribute"
                  }
                },
                "named": true,
                "value": "attributes"
              }
            }
          ]
        }
      ]
    },
    "attribute": {
      "type": "SEQ",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "."
            },
            {
              "type": "FIELD",
              "name": "id",
              "content": {
                "type": "SYMBOL",
                "name": "identifier"
              }
            }
          ]
        },
        {
          "type": "PATTERN",
          "value": " *"
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "value",
              "content": {
                "type": "SYMBOL",
                "name": "pattern"
              }
            },
            {
              "type": "SYMBOL",
              "name": "_pattern_skip"
            }
          ]
        }
      ]
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-zA-Z][a-zA-Z0-9_-]*"
    },
    "term": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "id",
          "content": {
            "type": "SYMBOL",
            "name": "term_identifier"
          }
        },
        {
          "type": "PATTERN",
          "value": " *"
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "FIELD",
          "name": "value",
          "content": {
            "type": "SYMBOL",
            "name": "pattern"
          }
        }
      ]
    },
    "term_identifier": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "-"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          },
          "named": false,
          "value": "identifier"
        }
      ]
    },
    "variable": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "$"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          },
          "named": false,
          "value": "identifier"
        }
      ]
    },
    "pattern": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_pattern_start"
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "pure_text"
              },
              {
                "type": "SYMBOL",
                "name": "placeable"
              }
            ]
          }
        },
        {
          "type": "SYMBOL",
          "name": "_pattern_end"
        }
      ]
    },
    "positional_arguments": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "expression"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "ALIAS",
                "content": {
                  "type": "TOKEN",
                  "content": {
                    "type": "PREC",
                    "value": 1,
                    "content": {
                      "type": "PATTERN",
                      "value": "[ \\n]*,[ \\n]*"
                    }
                  }
                },
                "named": false,
                "value": ","
              },
              {
                "type": "SYMBOL",
                "name": "expression"
              }
            ]
          }
        }
      ]
    },
    "named_argument": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "id",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "TOKEN",
            "content": {
              "type": "PREC",
              "value": 1,
              "content": {
                "type": "PATTERN",
                "value": "[ \\n]*:[ \\n]*"
              }
            }
          },
          "named": false,
          "value": ":"
        },
        {
          "type": "FIELD",
          "name": "value",
          "content": {
            "type": "SYMBOL",
            "name": "expression"
          }
        }
      ]
    },
    "named_arguments": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "named_argument"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "ALIAS",
                "content": {
                  "type": "TOKEN",
                  "content": {
                    "type": "PREC",
                    "value": 1,
                    "content": {
                      "type": "PATTERN",
                      "value": "[ \\n]*,[ \\n]*"
                    }
                  }
                },
                "named": false,
                "value": ","
              },
              {
                "type": "SYMBOL",
                "name": "named_argument"
              }
            ]
          }
        }
      ]
    },
    "function_call": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_blank_lines"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "ALIAS",
              "content": {
                "type": "PATTERN",
                "value": "[ \\n]*\\([ \\n]*"
              },
              "named": false,
              "value": "("
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SEQ",
                  "members": [
                    {
                      "type": "CHOICE",
                      "members": [
                        {
                          "type": "SEQ",
                          "members": [
                            {
                              "type": "SYMBOL",
                              "name": "positional_arguments"
                            },
                            {
                              "type": "SYMBOL",
                              "name": "_end_positional_args"
                            }
                          ]
                        },
                        {
                          "type": "BLANK"
                        }
                      ]
                    },
                    {
                      "type": "CHOICE",
                      "members": [
                        {
                          "type": "SYMBOL",
                          "name": "named_arguments"
                        },
                        {
                          "type": "BLANK"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "BLANK"
                }
              ]
            },
            {
              "type": "ALIAS",
              "content": {
                "type": "PATTERN",
                "value": "[ \\n]*\\)[ \\n]*"
              },
              "named": false,
              "value": ")"
            }
          ]
        }
      ]
    },
    "function_reference": {
      "type": "SEQ",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "ALIAS",
              "content": {
                "type": "FIELD",
                "name": "id",
                "content": {
                  "type": "TOKEN",
                  "content": {
                    "type": "PREC",
                    "value": 1,
                    "content": {
                      "type": "PATTERN",
                      "value": "[A-Z][A-Z0-9_-]*"
                    }
                  }
                }
              },
              "named": true,
              "value": "function_name"
            },
            {
              "type": "PATTERN",
              "value": " *"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "function_call"
        }
      ]
    },
    "message_reference": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "id",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": "."
                },
                {
                  "type": "FIELD",
                  "name": "attribute",
                  "content": {
                    "type": "SYMBOL",
                    "name": "identifier"
                  }
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "term_reference": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "id",
          "content": {
            "type": "SYMBOL",
            "name": "term_identifier"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": "."
                },
                {
                  "type": "FIELD",
                  "name": "attribute",
                  "content": {
                    "type": "SYMBOL",
                    "name": "identifier"
                  }
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "function_call"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "expression": {
      "type": "CHOICE",
      "members": [
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "placeable"
          },
          "named": true,
          "value": "inline_placeable"
        },
        {
          "type": "SYMBOL",
          "name": "number_literal"
        },
        {
          "type": "SYMBOL",
          "name": "string_literal"
        },
        {
          "type": "SYMBOL",
          "name": "message_reference"
        },
        {
          "type": "SYMBOL",
          "name": "term_reference"
        },
        {
          "type": "SYMBOL",
          "name": "variable"
        },
        {
          "type": "PREC",
          "value": 1,
          "content": {
            "type": "SYMBOL",
            "name": "function_reference"
          }
        }
      ]
    },
    "selector_expression": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "number_literal"
        },
        {
          "type": "SYMBOL",
          "name": "string_literal"
        },
        {
          "type": "SYMBOL",
          "name": "variable"
        },
        {
          "type": "PREC",
          "value": 1,
          "content": {
            "type": "SYMBOL",
            "name": "function_reference"
          }
        }
      ]
    },
    "_selector_key": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "SYMBOL",
          "name": "number_literal"
        }
      ]
    },
    "selector_variant": {
      "type": "SEQ",
      "members": [
        {
          "type": "ALIAS",
          "content": {
            "type": "PATTERN",
            "value": "\\*?\\[[ \\n]*"
          },
          "named": false,
          "value": "["
        },
        {
          "type": "FIELD",
          "name": "key",
          "content": {
            "type": "SYMBOL",
            "name": "_selector_key"
          }
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "PATTERN",
            "value": "[\\n ]*\\]"
          },
          "named": false,
          "value": "]"
        },
        {
          "type": "FIELD",
          "name": "value",
          "content": {
            "type": "SYMBOL",
            "name": "pattern"
          }
        }
      ]
    },
    "selectors": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": " *-> *\\n[ \\n]*"
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "SYMBOL",
            "name": "selector_variant"
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "placeable": {
      "type": "SEQ",
      "members": [
        {
          "type": "ALIAS",
          "content": {
            "type": "PATTERN",
            "value": "\\{[ \\n]*"
          },
          "named": false,
          "value": "{"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "expression"
                },
                {
                  "type": "ALIAS",
                  "content": {
                    "type": "PATTERN",
                    "value": "[ \\n]*\\}"
                  },
                  "named": false,
                  "value": "}"
                }
              ]
            },
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "selector_expression"
                },
                {
                  "type": "SYMBOL",
                  "name": "selectors"
                }
              ]
            }
          ]
        }
      ]
    },
    "number_literal": {
      "type": "PATTERN",
      "value": "\\d\\d*(\\.\\d+)?"
    },
    "string_literal": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\""
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "REPEAT",
            "content": {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "escaped_literal"
                },
                {
                  "type": "PATTERN",
                  "value": "[^\\\\\"]+"
                }
              ]
            }
          },
          "named": true,
          "value": "text"
        },
        {
          "type": "STRING",
          "value": "\""
        }
      ]
    },
    "escaped_literal": {
      "type": "CHOICE",
      "members": [
        {
          "type": "PATTERN",
          "value": "\\\\u[0-9a-fA-F]{4}"
        },
        {
          "type": "PATTERN",
          "value": "\\\\U[0-9a-fA-F]{6}"
        },
        {
          "type": "STRING",
          "value": "\\\""
        },
        {
          "type": "STRING",
          "value": "\\\\"
        }
      ]
    }
  },
  "extras": [],
  "conflicts": [],
  "precedences": [],
  "externals": [
    {
      "type": "SYMBOL",
      "name": "_pattern_start"
    },
    {
      "type": "SYMBOL",
      "name": "pure_text"
    },
    {
      "type": "SYMBOL",
      "name": "_pattern_end"
    },
    {
      "type": "SYMBOL",
      "name": "_pattern_skip"
    },
    {
      "type": "SYMBOL",
      "name": "_blank_lines"
    },
    {
      "type": "SYMBOL",
      "name": "unfinished_line"
    },
    {
      "type": "SYMBOL",
      "name": "_close_comment_block"
    },
    {
      "type": "SYMBOL",
      "name": "_end_positional_args"
    }
  ],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}