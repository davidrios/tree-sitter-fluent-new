{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "fluent",
  "rules": {
    "debug": {
      "type": "REPEAT",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "repeated_lines"
          },
          {
            "type": "STRING",
            "value": "a"
          },
          {
            "type": "STRING",
            "value": "aaa"
          }
        ]
      }
    },
    "repeated_lines": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "REPEAT1",
        "content": {
          "type": "SYMBOL",
          "name": "_blank_line"
        }
      }
    },
    "source_file": {
      "type": "SEQ",
      "members": [
        {
          "type": "REPEAT",
          "content": {
            "type": "PATTERN",
            "value": "\\s"
          }
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_definition"
          }
        }
      ]
    },
    "_comment_content": {
      "type": "PATTERN",
      "value": "[^\\n]+"
    },
    "comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "#"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_comment_content"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "group_comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "##"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_comment_content"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "file_comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "###"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_comment_content"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "_definition": {
      "type": "CHOICE",
      "members": [
        {
          "type": "PREC_LEFT",
          "value": 1,
          "content": {
            "type": "SYMBOL",
            "name": "comment"
          }
        },
        {
          "type": "PREC_LEFT",
          "value": 2,
          "content": {
            "type": "SYMBOL",
            "name": "message"
          }
        },
        {
          "type": "PREC_LEFT",
          "value": 3,
          "content": {
            "type": "SYMBOL",
            "name": "message_with_comment"
          }
        }
      ]
    },
    "message": {
      "type": "PREC_LEFT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "identifier"
              },
              {
                "type": "SYMBOL",
                "name": "term"
              }
            ]
          },
          {
            "type": "REPEAT",
            "content": {
              "type": "SYMBOL",
              "name": "_space_tabs"
            }
          },
          {
            "type": "STRING",
            "value": "="
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "SEQ",
                "members": [
                  {
                    "type": "SYMBOL",
                    "name": "text"
                  },
                  {
                    "type": "REPEAT",
                    "content": {
                      "type": "SYMBOL",
                      "name": "_nl_or_indented"
                    }
                  }
                ]
              },
              {
                "type": "REPEAT1",
                "content": {
                  "type": "SYMBOL",
                  "name": "_nl_or_indented"
                }
              }
            ]
          }
        ]
      }
    },
    "message_comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "#"
        },
        {
          "type": "PATTERN",
          "value": "[^#\\n]+"
        }
      ]
    },
    "message_with_comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "REPEAT1",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "message_comment"
              },
              {
                "type": "STRING",
                "value": "\n"
              }
            ]
          }
        },
        {
          "type": "SYMBOL",
          "name": "message"
        }
      ]
    },
    "_blank_line": {
      "type": "STRING",
      "value": "\n"
    },
    "_nl_or_indented": {
      "type": "CHOICE",
      "members": [
        {
          "type": "PREC_LEFT",
          "value": 0,
          "content": {
            "type": "REPEAT1",
            "content": {
              "type": "SYMBOL",
              "name": "_blank_line"
            }
          }
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_blank_line"
            },
            {
              "type": "REPEAT1",
              "content": {
                "type": "SYMBOL",
                "name": "_space_tabs"
              }
            },
            {
              "type": "SYMBOL",
              "name": "text"
            }
          ]
        }
      ]
    },
    "_identifier": {
      "type": "PATTERN",
      "value": "[a-z_][a-z0-9_-]*"
    },
    "identifier": {
      "type": "SYMBOL",
      "name": "_identifier"
    },
    "term": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "-"
        },
        {
          "type": "SYMBOL",
          "name": "_identifier"
        }
      ]
    },
    "text": {
      "type": "REPEAT1",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "PATTERN",
            "value": "[\t\\r -z|-퟿-�]+"
          },
          {
            "type": "SYMBOL",
            "name": "placeable"
          }
        ]
      }
    },
    "placeable": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_space_tabs"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "quoted_text"
            }
          ]
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_space_tabs"
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "quoted_escaped": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "\\u"
            },
            {
              "type": "SYMBOL",
              "name": "_hexdigit"
            },
            {
              "type": "SYMBOL",
              "name": "_hexdigit"
            },
            {
              "type": "SYMBOL",
              "name": "_hexdigit"
            },
            {
              "type": "SYMBOL",
              "name": "_hexdigit"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "\\U"
            },
            {
              "type": "SYMBOL",
              "name": "_hexdigit"
            },
            {
              "type": "SYMBOL",
              "name": "_hexdigit"
            },
            {
              "type": "SYMBOL",
              "name": "_hexdigit"
            },
            {
              "type": "SYMBOL",
              "name": "_hexdigit"
            },
            {
              "type": "SYMBOL",
              "name": "_hexdigit"
            },
            {
              "type": "SYMBOL",
              "name": "_hexdigit"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "\\\""
        },
        {
          "type": "STRING",
          "value": "\\\\"
        }
      ]
    },
    "quoted_text": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\""
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "quoted_escaped"
              },
              {
                "type": "PATTERN",
                "value": "[\t-\\n\\r -!#-\\[\\]-퟿-�]+"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "\""
        }
      ]
    },
    "_hexdigit": {
      "type": "PATTERN",
      "value": "[0-9a-fA-F]"
    },
    "_space_tabs": {
      "type": "PATTERN",
      "value": "[ \\t]"
    }
  },
  "extras": [],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}